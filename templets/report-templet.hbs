  <link href="https://kgate.bc.gov.sa/assets/SST-Arabic-Roman.ttf" rel="stylesheet" type="text/css">
  <style>
    @font-face {
      font-family: "SST-Arabic-Roman";
      src: url('https://kgate.bc.gov.sa/assets/SST-Arabic-Roman.ttf');
    }
    * { box-sizing: border-box; }
    body { margin: 0; padding: 0; font-family: "SST-Arabic-Roman"; }
    a[x-apple-data-detectors] { color: inherit !important; text-decoration: inherit !important; }
    #MessageViewBody a { color: inherit; text-decoration: none; }
    p { line-height: inherit; font-family: "SST-Arabic-Roman"; }
    span { font-family: "SST-Arabic-Roman"; }
    .desktop_hide, .desktop_hide table { mso-hide: all; display: none; max-height: 0px; overflow: hidden; }
    @media (max-width: 768px){ .color-white{ color: #787878 !important; } }
    @media (max-width:520px) {
      .row-content { width: 100% !important; }
      .mobile_hide { display: none; }
      .stack .column { width: 100%; display: block; }
      .mobile_hide { min-height: 0; max-height: 0; max-width: 0; overflow: hidden; font-size: 0px; }
      .desktop_hide, .desktop_hide table { display: table !important; max-height: none !important; }
    }
  </style>
</head>
